#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = Dllmem Usemem
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

Dllmem: Dllmem.dpr
  $(DCC)

Usemem: Usemem.dpr
  $(DCC)


